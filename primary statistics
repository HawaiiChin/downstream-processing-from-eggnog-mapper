#counting all proteins
grep -c "^k141" emap40.emapper.annotations

#counting annotated proteins
awk '$21!="-" {count++} END {print count}'emap40.emapper.annotations

echo "scale=1; $annotated_proteins*100/$total_proteins" | bc)%)"

#extracting column5 for eggNOG_OGs taken as instance
cut -f5 emap40.emapper.annotations| grep -v "^#" | grep -v "^-$" | sort| uniq -c|sort -nr|wc -l

#KO pathways
awk -F"\t" '{split($13, pathways, ",");for (i in pathways) 
> print pathways[i]}' eggnog-mapper/emap40.emapper.annotations | grep "ko"|sort|uniq -c|sort -nr
